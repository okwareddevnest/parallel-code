{
  "name": "parallel-code-mcp-server",
  "version": "2.2.1",
  "description": "MCP server for enabling collaborative AI coding with conflict prevention",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "parallel-code-mcp": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
